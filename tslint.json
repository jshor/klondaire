{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "quotemark": [true, "single"],
    "semicolon": false,
    "space-before-function-paren": [true, "always"],
    "trailing-comma": [true, "never"]
  },
  "rulesDirectory": []
}
