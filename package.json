{
  "name": "klondaire",
  "description": "Klondike solitaire game for Windows, Linux, and macOS.",
  "version": "0.1.0",
  "author": "",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "deploy": "deploy-aws-s3-cloudfront --delete --non-interactive --bucket ${AWS_S3_BUCKET_NAME} --source ./dist/web --distribution ${AWS_CF_ID}",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "wotan -p tsconfig.json -c tslint.json",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "unit": "jest"
  },
  "build": {
    "productName": "klondaire",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "canvas-confetti": "^0.5.0",
    "is-mobile": "^2.1.0",
    "lodash-es": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lodash.shuffle": "^4.2.0",
    "uuid": "^3.3.3",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-smooth-dnd": "^0.8.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.2",
    "@fimbul/valtyr": "^0.21.0",
    "@fimbul/ve": "^0.21.0",
    "@fimbul/wotan": "^0.21.1",
    "@types/jest": "^24.0.23",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ajv": "^6.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^4.1.6",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "codecov": "^3.6.5",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "deploy-aws-s3-cloudfront": "^2.2.1",
    "devtron": "^1.4.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.21.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.15.3",
    "typescript": "^3.7.2",
    "url-loader": "^1.0.1",
    "vue-class-component": "^7.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
